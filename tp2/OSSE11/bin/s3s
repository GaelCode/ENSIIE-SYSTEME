#!/bin/bash

if [ $# -ne 3 ] ; then
    echo "Usage: $0 <str1> <str2> <str3>"
    exit 1
fi
if [ "$1" \> "$3" -a "$3" \> "$2" ] ; then
    echo $2 ; echo $3 ; echo $1
elif [ "$1" \> "$2" -a "$3" \> "$1" ]; then
    echo $2 ; echo $1 ; echo $3
elif [ "$2" \> "$3" -a "$3" \> "$1" ]; then
    echo $1 ; echo $3 ; echo $2
elif [ "$1" \> "$2" -a "$2" \> "$3" ] ; then
    echo $3 ; echo $2 ; echo $1
elif [ "$2" \> "$1" -a "$1" \> "$3" ] ; then
    echo $3 ; echo $1 ; echo $2
else
    echo $1 ; echo $2 ; echo $3
fi